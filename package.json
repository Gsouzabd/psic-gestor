{
  "name": "psic-gestor-monorepo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev:psic": "pnpm --filter psicgestor dev",
    "dev:estetic": "pnpm --filter esteticgestor dev",
    "build": "pnpm build:core && pnpm -r --filter './apps/*' build",
    "build:core": "pnpm --filter @gestor/core build",
    "build:psic": "pnpm build:core && pnpm --filter psicgestor build",
    "build:estetic": "pnpm build:core && pnpm --filter esteticgestor build",
    "preview:psic": "pnpm --filter psicgestor preview",
    "preview:estetic": "pnpm --filter esteticgestor preview",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r exec rm -rf node_modules dist && rm -rf node_modules"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
